{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "spindex.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/normal.txt",
      "destination": "/normal.txt"
    },
    {
      "source": "/(.*)",
      "destination": "/index.js"
    }
  ],
  "headers": [
    {
      "source": "/normal.txt",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}
